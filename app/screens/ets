//ExamScreen

import React, { useEffect, useState } from "react";

const [examQuestions, setExamQuestions] = useState("")
  
useEffect(() => {
  async function getExamQuestions() {
    const parseQuery = new Parse.Query("ExamQuestion");
    let result = await parseQuery.find();
    console.dir(result)
    setExamQuestions(result)
  }
  getExamQuestions();
})


//QuestionText

import React, { useEffect, useState } from "react";
import {View, Text} from "react-native"


export default function QuestionText(props) {
  const [examQuestionText, setExamQuestions] = useState("")

  useEffect(() => {
    async function getExamQuestions() {
      const parseQuery = new Parse.Query("ExamQuestion");
      let result = await parseQuery.find();
      //console.dir(result)
      setExamQuestions(result[props.qText].get("Question_Text"))
    }
    getExamQuestions();
  })

  return (
    <View>
      <Text>{examQuestionText}</Text>
    </View>
  );
}


//ExamQuestionScreen

const [examQuestions, setExamQuestions] = useState("")

useEffect(() => {
  async function getExamQuestions() {
    const parseQuery = new Parse.Query("ExamQuestion");
    let result = await parseQuery.find();
    //console.dir(result)
    setExamQuestions(result)
  }
  getExamQuestions();
})

