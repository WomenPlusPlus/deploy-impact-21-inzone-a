//JSON
const getJSONdata = async key => {
  try {
    let data = await AsyncStorage.getItem(key);
    if (data !== null) {
      console.log(JSON.parse(data));
      console.log('yes1')
      return JSON.parse(data);
    }
  } catch (error) {
    console.log(error);
  }
  console.log('yes')
};